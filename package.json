  },
  "homepage": "https://github.com/supabase/ssr#readme",
  "devDependencies": {
    "@eslint/js": "^9.3.0",
    "@supabase/supabase-js": "^2.43.4",
    "@vitest/coverage-v8": "^1.6.0",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "tsd": "^0.32.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.10.0",
    "vitest": "^1.6.0"
